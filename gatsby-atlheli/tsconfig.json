{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "jsx": "preserve",
    "lib": ["dom", "es2015", "es2017"],
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "typeRoots": ["types", "node_modules/@types"]
  },
  "include": ["**/*.ts", "**/*.tsx", "images.d.ts"]
}